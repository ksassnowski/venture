{
    "name": "sassnowski/venture",
    "type": "library",
    "description": "A package to manage complex workflows built on top of Laravel's queue.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kai Sassnowski",
            "email": "me@kai-sassnowski.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/bus": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/events": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/support": "^8.0",
        "opis/closure": "^3.5",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.15",
        "ergebnis/php-cs-fixer-config": "^3.2",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^6.23",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpunit/phpunit": "^9.5.8",
        "roave/security-advisories": "dev-latest",
        "spatie/phpunit-watcher": "^1.22",
        "vimeo/psalm": "^4.12"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sassnowski\\Venture\\VentureServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Sassnowski\\Venture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@composer normalize"
        ],
        "post-update-cmd": [
            "@composer normalize"
        ],
        "psalm": [
            "vendor/bin/psalm"
        ],
        "cs": [
            "mkdir -p .build/php-cs-fixer",
            "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --diff --verbose"
        ]
    }
}

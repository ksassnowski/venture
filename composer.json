{
    "name": "sassnowski/venture",
    "description": "A package to manage complex workflows built on top of Laravel's queue.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kai Sassnowski",
            "email": "me@kai-sassnowski.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/bus": "^9.0 || ^10.0",
        "illuminate/container": "^9.0 || ^10.0",
        "illuminate/events": "^9.0 || ^10.0",
        "illuminate/queue": "^9.0 || ^10.0",
        "illuminate/support": "^9.0 || ^10.0",
        "laravel/serializable-closure": "^1.2",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.29",
        "ergebnis/php-cs-fixer-config": "^5.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/larastan": "^2.4",
        "orchestra/testbench": "^7.0 || ^8.0",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpunit/phpunit": "^9.5.8",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.6 || ^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Sassnowski\\Venture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sassnowski\\Venture\\VentureServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer normalize"
        ],
        "post-update-cmd": [
            "@composer normalize"
        ],
        "analyze": [
            "vendor/bin/phpstan analyze -c phpstan.neon"
        ],
        "check": [
            "@cs",
            "@analyze",
            "@test"
        ],
        "cs": [
            "mkdir -p .build/php-cs-fixer",
            "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --diff --verbose"
        ],
        "test": [
            "vendor/bin/pest"
        ]
    }
}
